msgid "Balance"
msgstr "Điểm"

msgid "Sign up"
msgstr "Đăng ký"

msgid "Sign in with Facebook"
msgstr "Đăng nhập qua Facebook"

msgid "Sign in with Google+"
msgstr "Đăng nhập qua Google+"

msgid "Sign up with Facebook"
msgstr "Đăng ký qua Facebook"

msgid "Sign up with Google+"
msgstr "Đăng ký qua Google+"

msgid "Remember me"
msgstr "Nhớ mật khẩu"

msgid "Sign up New Account"
msgstr "Đăng ký tài khoản mới"

msgid "Please enter a valid email address."
msgstr "Vui lòng cung cấp địa chỉ email."

msgid "Please enter the correct email."
msgstr "Vui lòng cung cấp chính xác địa chỉ email."

msgid "Please enter the email address associated with your bestay account to receive a password reset link."
msgstr "Vui lòng nhập địa chỉ email được liên kết với tài khoản bestay để nhận liên kết đặt lại mật khẩu."

msgid "Reset password"
msgstr "Gửi"

msgid "Forgot password?"
msgstr "Quên mật khẩu?"

msgid "Please check your email to proceed with password change."
msgstr "Vui lòng kiểm tra email để tiến hành thay đổi mật khẩu."

msgid "Please enter a new password."
msgstr "Vui lòng nhập mật khẩu bạn muốn thay đổi."

msgid "New password"
msgstr "Mật khẩu mới"

msgid "Confirm password"
msgstr "Xác nhận mật khẩu"

msgid "Change password"
msgstr "Đổi mật khẩu"

msgid "Enter confirm password same as password"
msgstr "Mật khẩu không giống nhau"

msgid "Password must be at least 8 characters."
msgstr "Mật khẩu tối thiểu phải 8 ký tự."

msgid "Password must not exceed 50 characters."
msgstr "Mật khẩu không quá 50 ký tự."

msgid "The token is no longer valid, please perform the password reset function again."
msgstr "Mã token không còn hợp lệ, vui lòng thực hiện lại chức năng đặt lại mật khẩu."

msgid "Password changed successfully."
msgstr "Mật khẩu đã được thay đổi, vui lòng dùng mật khẩu mới để đăng nhập."

msgid "Wrong login information, incorrect password."
msgstr "Sai thông tin đăng nhập, mật khẩu không chính xác."

msgid "The mobile number is not correct."
msgstr "Số điện thoại không chính xác."

msgid "Email do not exits"
msgstr "Địa chỉ email không tồn tại trong hệ thống"
