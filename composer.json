{
    "name": "zendframework/skeleton-application",
    "description": "Skeleton Application for Zend Framework zend-mvc applications",
    "type": "project",
    "license": "BSD-3-Clause",
    "keywords": [
        "framework",
        "mvc",
        "zf"
    ],
    "homepage": "http://framework.zend.com/",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4",
        "ext-gd": "*",
        "ext-json": "*",
        "doctrine/doctrine-orm-module": "^2.1",
        "radnan/rdn-csv": "2.*",
        "beberlei/doctrineextensions": "^1.2",
        "laminas/laminas-component-installer": "^3.0",
        "ramsey/uuid": "^4.2",
        "laminas/laminas-dependency-plugin": "^2.2",
        "laminas/laminas-migration": "^1.3",
        "laminas/laminas-servicemanager": "^3.4",
        "laminas/laminas-json": "^3.0",
        "laminas/laminas-mvc": "^3.0.1",
        "laminas/laminas-cache": "^2.7.1",
        "laminas/laminas-authentication": "^2.7",
        "laminas/laminas-barcode": "^2.8",
        "laminas/laminas-captcha": "^2.9",
        "laminas/laminas-crypt": "^3.3",
        "laminas/laminas-db": "^2.8.1",
        "laminas/laminas-file": "^2.8",
        "laminas/laminas-log": "^2.9",
        "laminas/laminas-mvc-console": "^1.1.10",
        "laminas/laminas-i18n": "^2.9",
        "laminas/laminas-mvc-form": "^1.0",
        "laminas/laminas-mvc-i18n": "^1.0",
        "laminas/laminas-session": "^2.8",
        "laminas/laminas-mvc-plugin-flashmessenger": "^1.1",
        "laminas/laminas-mvc-plugins": "^1.0.1",
        "laminas/laminas-navigation": "^2.9",
        "laminas/laminas-psr7bridge": "^0.2.2",
        "laminas/laminas-servicemanager-di": "^1.0",
        "laminas/laminas-stratigility": "^3.2",
        "laminas/laminas-dom": "^2.7",
        "laminas/laminas-development-mode": "^3.0",
        "intervention/image": "^2.7",
        "phpoffice/phpspreadsheet": "^1.30"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src/",
            "Admin\\": "module/Admin/src/",
            "Sulde\\": "module/Sulde/src/",
            "Users\\": "module/Users/src/",
            "Product\\": "module/Product/src/",
            "Purchase\\": "module/Purchase/src/",
            "Supplier\\": "module/Supplier/src/",
            "Customer\\": "module/Customer/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApplicationTest\\": "module/Application/test/"
        }
    },
    "extra": [],
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "development-disable": "zf-development-mode disable",
        "development-enable": "zf-development-mode enable",
        "development-status": "zf-development-mode status",
        "post-create-project-cmd": [
            "@development-enable"
        ],
        "serve": "php -S 0.0.0.0:8080 -t public public/index.php",
        "test": "phpunit"
    },
    "require-dev": {
        "laminas/laminas-developer-tools": "^1.1.0",
        "laminas/laminas-test": "^3.0.1"
    },
    "config": {
        "allow-plugins": {
            "laminas/laminas-component-installer": true,
            "laminas/laminas-dependency-plugin": true
        }
    }
}
